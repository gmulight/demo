/* lc-tabpage */
.lc-tab-page {
  display:flex;
  background-color:#F9F9F9;
  width: 750rpx;
  flex-direction: column;
  position: absolute;
}

.tab-title-list {
  display:flex;
  flex-direction: row;
  background-color:#FFFFFF;
  height:80rpx;
  white-space: nowrap;
}

.title-item {
  display:flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  border-bottom-style: solid;
  position: relative;
  background-color:#FFFFFF;
  width:150rpx;
  height:80rpx;
  display: inline-block;
}
.text-wrap{
  display:flex;
  position:relative;
  justify-content: center;
  align-items: center;
  width:150rpx;
  height:80rpx;
}
.tab-tip{
  display:flex;
  position: absolute;
  justify-content: center;
  align-items: center;
  text-align: center;
}

.title-item-normal {
  display:flex;
  flex: 1;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  letter-spacing: 0.09rpx;
  line-height: 26rpx;
  border-bottom-width: 1rpx;
  border-bottom-style: solid;
  border-bottom-color: rgba(0,0,0,0.2);
}

.title-item-normal-border {
  border-left-style: solid;
  border-left-width: 1rpx;
  border-left-color: rgba(0,0,0,0.2);
}

.tab-title-list-card {
  display:flex;
  flex-direction: row;
  border-bottom-width: 1rpx;
  border-bottom-style: solid;
  border-bottom-color: rgba(0,0,0,0.2);
}

.title-item-card-wrap {
  display:flex;
  width: 690rpx;
  /* height: 58rpx; */
  flex-direction: row;
  align-items: center;
  justify-content: center;
  /* background-color: #FFFFFF; */

  border-top-style: solid;
  border-top-width: 1rpx;
  border-top-color: #C8C8C8;
  
  border-right-style: solid;
  border-right-width: 1rpx;
  border-right-color: #C8C8C8;

  border-bottom-style: solid;
  border-bottom-width: 1rpx;
  border-bottom-color: #C8C8C8;

  border-left-style: solid;
  border-left-color: #C8C8C8;
  border-left-width: 1rpx;  

  border-top-left-radius: 8rpx; 
  border-top-right-radius: 8rpx;
  border-bottom-left-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}

.title-item-card {
  display:flex;
  flex: 1;
  height: 56rpx;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  letter-spacing: 0.08rpx;
  line-height: 26rpx;
}
.bottom_line_height {
  height: 1rpx;
}
  .title-item-card-border {
  border-left-style: solid;
  border-left-width: 1rpx;
  border-left-color: #c8c8c8;
}

.border-bottom {
  position: absolute;
  bottom: 0;
  width: 150rpx;
  height: 6rpx;
}

.tab-page-wrap {
  width: 750rpx;
  overflow: hidden;
  position: relative;
}

.tab-container {
  display:flex;
  flex-direction: row;
  white-space: nowrap
}
.slideAnimation {
  position: absolute;
  transition: all .4s ease;
}

.tab-text {
  lines: 1;
  -webkit-line-clamp: 1;
  text-overflow: ellipsis;
  font-size:32rpx;
  padding:0 10rpx;
}
