/*上传图片*/

/* 预览 */

.up_img_box {
  width: 670rpx;
  display: flex;
  flex-wrap: wrap;
  padding: 0 40rpx;
}

.title-img {
  width: 670rpx;
  text-align:left;
   font-size: 32rpx;
   color: #3B6BFA;
   margin-bottom:40rpx;
}

.preview-warp {
  display: flex;
  margin-right: 14rpx;
  position: relative;
}

.preview-warp image {
  width: 184rpx;
  height: 184rpx;
  margin-bottom: 14rpx;
}

.preview-warp icon {
  position: absolute;
  right: -10rpx;
  top: -10rpx;
  color: #fff;
  background: #fff;
  border-radius: 50%;
}

/* 上传按钮组 */

.upimg {
  width: 184rpx;
  height: 184rpx;
  display: flex;
  align-content: center;
  justify-content: center;
  flex-wrap: wrap;
}

.upimg-copy {
  width: 184rpx;
  height: 184rpx;
  border: 1px solid #eee;
  display: flex;
  align-content: center;
  justify-content: center;
  flex-wrap: wrap;
  box-shadow: 0px 1rpx 16rpx 0px rgba(28, 28, 28, 0.14);
  border-radius: 10rpx;
  background: #F5F5F5;
}

.add_img {
  width: 184rpx;
  height:  184rpx;
  display: block;
}

.addText {
  font-size: 24rpx;
  color: #a8a8a8;
  width: 100%;
  text-align: center;
  margin-top: 10rpx;
}

.btns {
  margin-top: 80rpx;
  width: 670rpx;
  display: flex;
  justify-content: center;
  align-items:center;
  flex-direction: row;
}

.btn-item image {
  width: 300rpx;
  height:80rpx;
}
.checked-img, img-box {
    width: 200rpx;
    height: 200rpx;
}
.img-box {
    position: relative;
    padding: 9rpx;
}
.upload {
  width:670rpx;
    
    display:flex;
    flex-direction: row;
    flex-wrap: wrap;
    
}
.delete-img{
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 40rpx;
    height: 40rpx;
}
.upload-tip {
    position: absolute;
    top: 100%;
    left: 0;
    color: #666666;
    font-size: 22rpx;
        left: 10%;
}
