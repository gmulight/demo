}
.log-item {
  margin: 10rpx;
}
.customPlaClass {
  color: #ccc;
  font-size:24rpx;
  line-height: 24rpx;
}

.sign-item-no1 {
  width:670rpx;
  display: flex;
  flex-direction: row;
  font-size: 32rpx;
  border-bottom: 1px solid #f5f5fc;
  justify-content: space-between;
  align-items:center;
  height: 100rpx;
}
.sign-item {
  width:670rpx;
