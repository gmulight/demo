.container {
  background: #FEC208;
  padding: 26rpx;
  font-family: "PingFangSC-Regular", sans-serif;
}
.kecheng-con{
    width: 100%;
    height: 77%;
    display: flex;
    flex-flow: column;
        justify-content:center;
}
.file-con {
  background: #fff;
  border-radius: 16rpx;
  padding: 26rpx;
  margin-bottom: 4%;
}
.ke-detail {
  background:url('../../images/ke.png') no-repeat;
  background-size: 100% 100%;
}
.file-logo {
  width: 64rpx;
  height: 50rpx;
  margin-bottom: 6px;
  background:url('../../images/file.png') no-repeat;
  background-size: 100% 100%;
}
.demo {
  background:url('../../images/d.png') no-repeat;
  background-size: 100% 100%;
}
.kecheng-title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  cursor: pointer;
}
.kecheng {
   width: 64rpx;
  height: 56rpx;
  margin-bottom: 6px;
  background:url('../../images/kecheng.png') no-repeat;
  background-size: 100% 100%;
}
.kecheng-more {
  height: 30rpx;
  width: 72rpx;
  background:url('../../images/gengduo.png') no-repeat;
  background-size: 100% 100%;
}

.con-hrefs {
  position: relative;
  align-items:center;
  color: #3F3D37;
  font-size:28rpx;
  margin-bottom: 16rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.href-info {
  position: absolute;
  width: 350rpx;
  left: 0;
}
.pointer {
  position: absolute;
  left: 370rpx;
  width: 11.08rpx;
  height: 20.24rpx;
}
.href-copy {
  position: absolute;
  right: 0;
   text-decoration:underline;
  cursor:pointer
}
.userinfo {
  display: flex;
  justify-content: center;
      height: 33%;
    justify-content: center;
    align-items: center;
}
.btn-baoming {
  width: 570rpx;
  height: 80rpx;
  background:url('../../images/baoming.png') no-repeat;
  background-size: 100% 100%;
}
.con-kes {
   display:flex;
  flex-direction: column;
  margin-bottom: 16rpx;
}
.ke-item {

  height: 130rpx;
  display:flex;
  flex-direction: row;
  align-items: center;
}
.ke-item:first-child {
  margin-top: 20rpx;
  margin-bottom: 40rpx;
}
.ke-item-logo1 {
  width: 250rpx;
  height: 130rpx;
  background:url('../../images/kecheng1.png') no-repeat;
  background-size: 100% 100%;
  flex-shrink:0;
}
.ke-item-logo2 {
  width: 250rpx;
  height: 130rpx;
  background:url('../../images/kecheng2.png') no-repeat;
  background-size: 100% 100%;
  flex-shrink:0;
}
.ke-item-text {
  color: #333;
  font-size: 32rpx;
  margin-left: 30rpx;
  font-weight: 700;
}

